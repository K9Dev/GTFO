            <string>/*
Inventory Items
0 = Key
1 = Box
*/

//Display the inventory?
globalvar showInventory;
showInventory = true;

//Total Inventory Item Space
globalvar maxItems;
maxItems = 5;

//Array with Items
globalvar items;

//fill the array with -1 ( = empty)
for ( i = 0; i &lt; maxItems; i += 1 ){
    items[i] = -1;
}
